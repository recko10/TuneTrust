{
    "version": 2,
    "builds": [{
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python-3.8.18"
      }
    }],
    "routes": [{
      "src": "/(.*)",
      "dest": "manage.py"
    }],
    "env": {
      "DJANGO_SETTINGS_MODULE": "myproject.settings"
    }
  }
  